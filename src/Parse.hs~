{-# LANGUAGE OverloadedStrings #-}
module Press.Parse where

import Data.Attoparsec.ByteString

data Data = T  Texts
          | Mu Musics
          | Ma Maths
          | Append Data   Data
          deriving Show

{-
Append (T "Here is text")
       Append (Mu "a4 b4 c4")
              (Ma "1+2 = 3")
-}

type Texts  = String
type Musics = String
type Maths  = String

parseData :: Parser Data
parseData = do
  text <- takeByteString
  return (T $ show text)
